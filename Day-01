select first_name, last_name, gender from patients where gender='M';
select * from patients;
select count (*) from patients where year (birth_date) =2010;

select first_name, last_name, height from patients where height = (select max (height) from patients)
select * from patients where patient_id in ('1', '45', '534','879','1000')
select count(patient_id) as total_asmissions from admissions;
select * from admissions where admission_date = discharge_date;
select patient_id, count (admission_date) As total_admissions from admissions where patient_id = '579'
select distinct city from patients where province_id='NS';
select first_name, last_name, birth_date from patients where  height> 160 and weight > 70;
